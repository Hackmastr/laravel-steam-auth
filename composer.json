{
    "name": "hackmastr/laravel-steam-auth",
    "description": "Laravel Steam Auth",
    "keywords": ["steam", "laravel", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Brytkov",
            "email": "invis.nik@gmail.com"
        }
    ],
    "replace": {
	"invisnik/laravel-steam-auth": "4.4.0"
    },
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0.1",
        "illuminate/http": "^9.0|^10.0",
        "illuminate/routing": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Invisnik\\LaravelSteamAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Invisnik\\LaravelSteamAuth\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Invisnik\\LaravelSteamAuth\\SteamServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
